	box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
	/* margin-top: 12px; */
	padding: 8px 0;
	width: 100%;
}
.tt-suggestion {
	font-size: 22px;  /* Set suggestion dropdown font size */
	padding: 3px 20px;
}
.tt-suggestion:hover {
	cursor: pointer;
	background-color: #e06666;
	color: #FFFFFF;
}
.tt-suggestion p {
	margin: 0;
}
